<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ktlum">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KtLum"
        android:usesCleartextTraffic="true">
        <activity android:name=".controller.UserInfoActivity"></activity>
        <activity android:name=".controller.RoomDetails" />
        <activity android:name=".controller.RoomList" />
        <activity android:name=".controller.RoomListTemp" />
        <activity android:name=".controller.SignUp" />
        <activity android:name=".controller.BasicAuthInterceptor" />
        <activity android:name=".controller.FailLogin" />
        <activity android:name=".controller.SuccessLogin" />
        <activity android:name=".controller.BasicLogin" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>